<p id="notice"><%= notice %></p>

<% provide(:title, @user.name) %>
<h1>
  <%= link_to 'Users', users_path %> > <%= @user.name %>
</h1>
<table class="table table-striped">
  <tr>
    <td>Name</td>
    <td><%= @user.name %></td>
  </tr>
  <tr>
    <td>Email</td>
    <td><%= @user.email %></td>
  </tr>
</table>

<%= button_to 'Edit', edit_user_path(@user), :method => 'get', :class => 'btn btn-primary' %> |

